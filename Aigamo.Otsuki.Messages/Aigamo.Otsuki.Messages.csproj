<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <LangVersion>preview</LangVersion>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <Authors>ycanardeau</Authors>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/ycanardeau/Otsuki</PackageProjectUrl>
    <RepositoryUrl>https://github.com/ycanardeau/Otsuki</RepositoryUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <NoWarn>1701;1702;1591</NoWarn>
    <DocumentationFile>Aigamo.Otsuki.Messages.xml</DocumentationFile>
    <Description>DirectPlay 8 Reliable Protocol messages and DirectPlay 8 Core and Service Providers Protocol messages.</Description>
    <Version>1.1.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Aigamo.Extensions.Primitives" Version="1.1.0" />
  </ItemGroup>

</Project>
